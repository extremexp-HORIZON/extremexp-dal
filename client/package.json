{
  "name": "levis-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "js": "webpack",
    "watch-js": "webpack --watch",
    "css": "npm-run-all --sequential css-compile css-minify",
    "css-compile": "sass --quiet-deps --style expanded --source-map --embed-sources --precision 6 src/scss/expvis.scss dist/ivis.css",
    "css-minify": "cleancss -O1 --source-map --source-map-inline-sources --output dist/ivis.min.css dist/ivis.css",
    "watch-css": "nodemon --watch ../ivis-core/client/src/scss -e scss -x \"npm run css\"",
    "watch": "node ./pre-build.js && npm-run-all --parallel watch-css watch-js",
    "build": "node ./pre-build.js && npm-run-all --parallel css js"
  },
  "author": "Tomas Bures",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-proposal-function-bind": "^7.23.3",
    "@babel/plugin-transform-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "@coreui/coreui": "^5.0.1",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "bootstrap": "^5.3.3",
    "clean-css-cli": "^5.6.3",
    "css-loader": "^5.2.7",
    "file-loader": "^6.2.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.70.0",
    "sass-loader": "^10.5.2",
    "slugify": "^1.6.6",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "optionalDependencies": {},
  "dependencies": {
    "mxgraph": "^4.2.2",
    "net": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reconnecting-websocket": "^4.4.0",
    "slugify": "^1.6.6",
    "vscode-ws-jsonrpc": "^3.3.1"
  }
}
